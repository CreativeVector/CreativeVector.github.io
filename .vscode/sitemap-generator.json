{
  "sitemap.xml": {
    "Protocol": "https",
    "DomainName": "creativevector.store",
    "Root": "./",
    "IncludeExt": [
      ".html",
      ".php"
    ],
    "Exclude": [
      "404.html",
      "cancel.html",
      "success.html",
      "products/product.html",
      "auth.html",
      "checkout.html",
      "profile.html",
      "verify-success.html",
      "reset-password.html",
      "template/product-template.html"
    ],
    "TagsToInclude": [
      "priority",
      "lastmod"
    ],
    "DefaultChangeFrequency": "monthly",
    "TabCharacters": "\t",
    "bIncludeWWW": false,
    "bRemoveFileExtentions": false,
    "bUseTrailingSlash": false,
    "bAutomaticallyUpdateSitemap": true,
    "bMinimized": false
  }
}